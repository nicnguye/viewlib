{
  "name": "viewlib",
  "version": "1.0.0",
  "description": "Viewlib Test",
  "main": "server.js",
  "scripts": {
    "test": "mocha 'test/apiTest.js'",
    "sequelize-skeleton": "./node_modules/sequelize-cli/lib/sequelize init:models; ./node_modules/sequelize-cli/lib/sequelize init:config",
    "sequelize-init": "./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "start:dev": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicnguye/viewlib.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicnguye/viewlib/issues"
  },
  "homepage": "https://github.com/nicnguye/viewlib#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "sequelize": "^5.15.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0",
    "supertest": "^4.0.2"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "brace-style": [
        "error",
        "1tbs"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ],
      "object-shorthand": [
        "off"
      ],
      "consistent-return": [
        "off"
      ],
      "func-names": [
        "off"
      ],
      "no-console": [
        "off"
      ],
      "import/no-extraneous-dependencies": [
        "off"
      ],
      "import/no-unresolved": [
        "off"
      ]
    }
  }
}
